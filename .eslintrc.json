{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb"],

  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-undef": 0,
    "no-console": 0,
    "eqeqeq": 0,
    "consistent-return": 0,
    "no-underscore-dangle": 0,
    "no-useless-escape": 0,
    "prefer-rest-params": 0,
    "func-names": 0,
    "no-irregular-whitespace": 0,
    "max-len": 0
  }
}
